{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "app/components/UserNav.tsx",
    "app/components/Navbar.jsx",
    "tailwind.config.js",
    "app/api/auth/creation/route.js",
    "app/create/[id]/address",
    "app/page.jsx",
    "app/page.jsx"
, "app/components/HomeMap.jsx", "app/components/SelectCalendar.jsx", "app/components/SearchComponent.tsx"  ],
  "exclude": ["node_modules"]
}
